{
  "name": "SMp3Play",
  "version": "1.0.0",
  "description": "Simple music player mp3 ytdl application.",
  "main": "./dist/main.js",
  "scripts": {
    "dev:electron": "NODE_ENV=development webpack --display-modules --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "build:electron": "NODE_ENV=production webpack --config webpack.electron.config.js --mode production",
    "build:react": "NODE_ENV=production webpack --config webpack.react.config.js --mode production",
    "build": "yarn build:electron && yarn build:react",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "productName": "SMp3Play",
    "appId": "tk.smcodes.smp3play",
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Audio"
    }
  },
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^2.2.2",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "electron-db": "^0.15.7",
    "polished": "^4.1.2",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-progressive-graceful-image": "^0.6.12",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.3",
    "sweetalert2": "^10.16.2",
    "url-loader": "^4.1.1",
    "use-debounce": "^6.0.1",
    "uuid": "^8.3.2",
    "v8-compile-cache": "^2.3.0",
    "ytdl-core": "git+https://github.com/cchen028/node-ytdl-core.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/react": "^17.0.0",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SMCodesP/SMp3Play.git"
  },
  "keywords": [],
  "author": "Samuel Pereira da Silva <samuel.silva513@etec.sp.gov.br> (https://smcodes.tk/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SMCodesP/SMp3Play/issues"
  },
  "homepage": "https://github.com/SMCodesP/SMp3Play#readme"
}
